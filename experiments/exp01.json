{
    "traits" : {
        "Agreeableness": "agreeab",
        "Extroversion": "extrov",
        "Conscientiousness": "consc",
        "Emotional Stability": "emstab",
        "Openness": "open" 
    },

	"dataset": {
		"eeg_path": "data//raw//", 
        "personality_path": "data//Participants_Personality.xlsx",
        "sample_frequency": 128,
        "window_length": 3,
        "window_overlap": 0,
        "preprocess": true,
        "bandpass":[1,45],
        "scale": true,
        "split": [70,15,15],
        "save_path": "data//",
        "save_suffix": "_bpscaled"
	},

    "train": {
        "experiment_desc": "EEGNet-8,2_Agreeableness_exp01",
        "trait": "Agreeableness",
        "epochs": 5,
        "early_stop_epochs": 100,
        "learning_rate": 0.001,
        "batch_size": 64,
        "dropout": 0.01,
        "dropout_type": "Dropout",
        "temporal_filters": 4,
        "spatial_filters_depth": 2,
        "optimizer_metric": ["accuracy"],
        "save_path": "reports//",
        "save_best_model": false,
        "model_save_path": "models//",
        "model_save_metric": "val_loss",
        "save_figure": true
    }
}